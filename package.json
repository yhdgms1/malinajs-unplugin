{
  "name": "malinajs-unplugin",
  "version": "0.2.2",
  "description": "Transform Malina.js files to plain js",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "rollup",
    "esbuild",
    "malina",
    "malinajs"
  ],
  "homepage": "https://github.com/Artemis69/malinajs-unplugin#readme",
  "bugs": {
    "url": "https://github.com/Artemis69/malinajs-unplugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artemis69/malinajs-unplugin.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "format": "prettier --write ."
  },
  "dependencies": {
    "malinajs": "^0.6.64",
    "picomatch": "^2.3.1",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.27",
    "@types/picomatch": "^2.3.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "malinajs": "*"
  }
}
